@model List<Models.EF.DocumentCategory>
<div class="header_bottom_left">
    <div class="categories">
        <ul>
            <h3>Categories</h3>
            @foreach (var item in Model)
            {
                <li><a href="/tai-lieu/@item.MetaTitle-@item.ID">@item.Name</a></li>
            }
        </ul>
    </div>
</div>